{
  "name": "discord-bot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "updates:check": "npm-check-updates",
    "updates:apply": "npm-check-updates -u",
    "dev": "nodemon -e ts --exec \"yarn debug\"",
    "debug": "tsc && node --inspect=0.0.0.0 build/server.js",
    "start": "nodemon build/server.js"
  },
  "dependencies": {
    "discord.js": "https://github.com/vaporox/discord.js#fix-await-interaction",
    "emoji-regex": "^9.2.2"
  },
  "devDependencies": {
    "@types/emoji-regex": "^9.2.0",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "commander": "^7.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-sonarjs": "^0.7.0",
    "eslint-plugin-unicorn": "^33.0.1",
    "eslint-plugin-unused-imports": "^1.1.1",
    "nodemon": "^2.0.7",
    "npm-check-updates": "^11.6.0",
    "prettier": "^2.3.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  }
}
